{
  "name": "shared",
  "version": "0.0.0",
  "description": "Shared functions",
  "type": "module",
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "build": "tsup",
    "watch": "tsup -w"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.1",
    "esbuild": "^0.14.25",
    "esbuild-plugin-eslint": "^0.0.1",
    "eslint": "^8.10.0",
    "tsup": "^5.11.13",
    "type-fest": "^2.12.0",
    "typescript": "^4.6.0"
  },
  "tsup": {
    "dts": true,
    "entry": [
      "./src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "target": "node12",
    "clean": true
  }
}
