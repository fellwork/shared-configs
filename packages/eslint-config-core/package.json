{
  "name": "@fellwork/eslint-config-core",
  "version": "0.0.0",
  "description": "Shareable ESLint JS, YML, and JSON configuration",
  "keywords": [
    "Javascript",
    "YML",
    "JSON",
    "ESLint",
    "config"
  ],
  "homepage": "https://github.comfellwork/shared-configs#readme",
  "license": "MIT",
  "repository": "fellwork/shared-configs",
  "author": {
    "name": "Shane McGuirt",
    "email": "shane@fellwork.com"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "watch": "tsup -w",
    "version": "standard-version",
    "std-first": "standard-version --first-release",
    "std-pre": "standard-version --prerelease",
    "push": "git push --follow-tags",
    "release": "yarn version && yarn push && npm publish",
    "pre-release": "yarn std-pre && yarn push && npm publish"
  },
  "peerDependencies": {
    "eslint": ">=7.4.0",
    "prettier": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/eslint-parser": "~7.17.0",
    "@rushstack/eslint-patch": "~1.1.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "~2.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsonc": "^2.2.1",
    "eslint-plugin-n": "^15.0.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-unicorn": "^41.0.0",
    "eslint-plugin-yml": "^0.14.0",
    "jsonc-eslint-parser": "^2.1.0",
    "yaml-eslint-parser": "^0.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@types/babel__core": "^7.1.18",
    "@types/eslint": "^8.4.1",
    "@types/node": "^17.0.21",
    "@types/prettier": "~2.4.4",
    "esbuild": "^0.14.25",
    "esbuild-plugin-eslint": "^0.0.1",
    "eslint": "^8.10.0",
    "is-what": "~4.1.7",
    "prettier": "^2.5.1",
    "standard-version": "~9.3.2",
    "ts-toolbelt": "~9.6.0",
    "tslib": "~2.3.1",
    "tsup": "^5.11.13",
    "typescript": "~4.6.0"
  }
}
